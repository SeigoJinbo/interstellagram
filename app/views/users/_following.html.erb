<div class="modal fade" id="userFollowing" tabindex="-1" role="dialog" aria-labelledby="userFollowingTitle" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content" style="width: 90vw; margin: 0 auto; background-color: rgba(0, 53, 77, 0.9);">
			<div class="modal-header">
				<h5 class="modal-title" id="userFollowingTitle">Modal title</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				following
				<% @user.followings.each do |user| %>
					<%= user.user_name %>
				<% end %>
			</div>
			<div class="modal-footer">
				<div data-dismiss="modal"><%= render 'svgs/buttons/confirm_btn' %></div>
			</div>
		</div>
	</div>
</div>
